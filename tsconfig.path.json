{
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["node"],
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@constants/*": ["src/constants/*"],
      "@theme/*": ["src/theme/*"],
      "@interface/*": ["src/interface/*"],
      "@type/*": ["src/type/*"],
      "@utils/*": ["src/utils/*"],
      "@store/*": ["src/store/*"],
      "@api/*": ["src/api/*"],
      "@i18n/*": ["src/i18n/*"]
    }
  },
  "include": ["./src/react-app-env.d.ts"]
}
